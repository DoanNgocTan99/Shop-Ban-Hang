

<!DOCTYPE html>
<html lang="en">

<head>

    <!-- Basic Page Needs
    ================================================== -->
    <meta charset="utf-8">
    <title>Aviato | E-commerce template</title>

    <!-- Mobile Specific Metas
    ================================================== -->
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="description" content="Construction Html5 Template">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=5.0">
    <meta name="author" content="Themefisher">
    <meta name="generator" content="Themefisher Constra HTML Template v1.0">

    <!-- Favicon -->
    <link rel="shortcut icon" type="image/x-icon" href="~/assets/client/images/favicon.png" />

    <!-- Themefisher Icon font -->
    <link rel="stylesheet" href="~/assets/client/plugins/themefisher-font/style.css">
    <!-- bootstrap.min css -->
    <link rel="stylesheet" href="~/assets/client/plugins/bootstrap/css/bootstrap.min.css">

    <!-- Animate css -->
    <link rel="stylesheet" href="~/assets/client/plugins/animate/animate.css">
    <!-- Slick Carousel -->
    <link rel="stylesheet" href="~/assets/client/plugins/slick/slick.css">
    <link rel="stylesheet" href="~/assets/client/plugins/slick/slick-theme.css">

    <!-- Main Stylesheet -->
    <link rel="stylesheet" href="~/assets/client/css/style.css">

</head>

<body id="body">

    <section class="top-header">
        <div class="container">
            <div class="row">
                @{ Html.RenderAction("TopMenu", "Home");}
            </div>
        </div>
    </section>

    <section class="menu">
        <nav class="navbar navigation">
            <div class="container">
                @{ Html.RenderAction("MainMenu", "Home");}
            </div>
        </nav>
    </section>


    @RenderSection("Slider", required: false)

    @RenderSection("CategoryMenu", required: false)

    <section class="products section bg-gray">
        <div class="container">
            
            @RenderBody()
        </div>
    </section>

    @RenderSection("Action", required: false)

    <footer class="footer section text-center">
        <div class="container">
            <div class="row">
                @{ Html.RenderAction("Footer", "Home");}

            </div>
        </div>
    </footer>

    <!--
    Essential Scripts
    =====================================-->
    <!-- Main jQuery -->
    <script src="~/assets/client/plugins/jquery/dist/jquery.min.js"></script>
    <!-- Bootstrap 3.1 -->
    <script src="~/assets/client/plugins/bootstrap/js/bootstrap.min.js"></script>
    <!-- Bootstrap Touchpin -->
    <script src="~/assets/client/plugins/bootstrap-touchspin/dist/jquery.bootstrap-touchspin.min.js"></script>
    <!-- Instagram Feed Js -->
    <script src="~/assets/client/plugins/instafeed/instafeed.min.js"></script>
    <!-- Video Lightbox Plugin -->
    <script src="~/assets/client/plugins/ekko-lightbox/dist/ekko-lightbox.min.js"></script>
    <!-- Count Down Js -->
    <script src="~/assets/client/plugins/syo-timer/build/jquery.syotimer.min.js"></script>

    <!-- slick Carousel -->
    <script src="~/assets/client/plugins/slick/slick.min.js"></script>
    <script src="~/assets/client/plugins/slick/slick-animation.min.js"></script>

    <!-- Google Mapl -->
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCC72vZw-6tGqFyRhhg5CkF2fqfILn2Tsw"></script>
    <script type="text/javascript" src="~/assets/client/plugins/google-map/gmap.js"></script>

    <!-- Main Js File -->
    <script src="~/assets/client/js/script.js"></script>

</body>

</html>